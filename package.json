{
  "name": "webpack",
  "version": "1.0.0",
  "private": true,
  "description": "学习webpack增量打包多页面",
  "scripts": {
    "analyze": "cross-env NODE_ENV=production npm_config_report=true npm run build",
    "build": "cross-env NODE_ENV=production vue-cli-service build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "git cz",
    "commitlint": "commitlint --config commitlint.config.js -e -V",
    "generate": "ts-node api-generator/index.ts",
    "lint": "vue-cli-service lint",
    "lint-staged": "lint-staged --config lint-staged.config.js",
    "lint:style": "stylelint **/*.{html,vue,css,sass,scss} --fix",
    "pack": "node deploy/check.js && vue-cli-service build && node deploy/tar.js",
    "prepare": "husky install",
    "serve": "vue-cli-service serve",
    "serve:mock": "cross-env VUE_APP_MOCK=true vue-cli-service serve",
    "tar": "node deploy/check.js && node deploy/tar.js",
    "test:unit": "vue-cli-service test:unit"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@ava/fetch": "^1.3.0",
    "@ava/tool": "^1.3.0",
    "@ava/upload": "^1.4.0",
    "ava-ui": "^1.2.2",
    "core-js": "^3.13.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0",
    "vuex-module-decorators": "^1.0.1"
  },
  "devDependencies": {
    "@ava/api-generator": "^1.1.1",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "babel-plugin-component": "^1.1.1",
    "chalk": "^4.1.0",
    "chokidar": "^3.5.2",
    "commitizen": "^4.2.1",
    "conventional-changelog-cli": "^2.1.0",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "mockjs": "^1.1.0",
    "prettier": "^1.19.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-resources-loader": "^1.3.3",
    "stylelint": "^13.7.2",
    "stylelint-config-recess-order": "^2.1.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "tar-fs": "^2.1.0",
    "ts-node": "^10.0.0",
    "typescript": "~3.9.3",
    "vue-cli-plugin-style-resources-loader": "^0.1.4",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-spritesmith": "^1.1.0",
    "zlib": "^1.0.5"
  }
}
